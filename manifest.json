{
  "name": "Brave Bookmark Manager",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Makes the shit the way I want",
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "bookmarks",
    "activeTab"
  ],
  "browser_action": {
    "default_icon": "images/get_started16.png"
  }
}
